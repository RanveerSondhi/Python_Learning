print("The Love Calculator is calculating your score...")
name1 = input() # What is your name?
name2 = input() # What is their name?

combined_names = name1 + name2 #combine both names
lover_names = combined_names.lower() #make combine name into lower letter

#count each letter in both names
t = lover_names.count("t")
r = lover_names.count("r")
u = lover_names.count("u")
e = lover_names.count("e")
first_digit = t + r + u + e

l = lover_names.count("l")
o = lover_names.count("o")
v = lover_names.count("v")
e = lover_names.count("e")
second_digit = l + o + v + e

#add both number like string but represent as integer.
score = int(str(first_digit) + str(second_digit))

if (score < 20):
  print(f"Your score is {score}, Booooo... find new love.")
elif (score >= 40) and (score <= 50):
  print(f"Your score is {score}, you are alright together.")
elif score > 90:
  print(f"Your score is {score}, Perfect Couple.")
else:
  print(f"Your score is {score}.")

